{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "module" : "ESNext",
    "moduleResolution": "Node",
    "types": ["vite/client","element-plus/global"],
    "lib": [ "ES6", "DOM" ],
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "exclude": ["node_modules"],
}
